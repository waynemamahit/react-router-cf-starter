    "@cloudflare/vite-plugin": "1.15.3",
    "@cloudflare/vitest-pool-workers": "0.10.11",
    "@react-router/dev": "7.9.6",
    "@tailwindcss/vite": "4.1.17",
    "@types/node": "24",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
